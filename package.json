    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.0.0-canary.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "istanbul": "^0.4.5",
